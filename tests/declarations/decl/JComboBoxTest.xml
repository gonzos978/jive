<?xml version="1.0" encoding="UTF-8"?>
<JWindow xmlns="org.aswing" xmlns:geom="org.aswing.geom" xmlns:e="empty" xmlns:haxe="http://haxe.org/">
    <haxe:Implements><![CDATA[jive.DataContextControllable<org.aswing.JComboBoxTestModel>]]></haxe:Implements>
        <content>
                <JPanel>
                    <layout>
                            <FlowLayout>
                                    <align>org.aswing.FlowLayout.LEFT</align>
                                    <hgap>30</hgap>
                                    <vgap>30</vgap>
                                    <margin>true</margin>
                            </FlowLayout>
                    </layout>
                    <JComboBox
                            selectedItem="{Binding selected mode=twoway}"
                            selectedIndex="{Binding index mode=twoway}"
                            editable="true">
                        <preferredSize>
                            <geom:IntDimension width="100" height="50"/>
                        </preferredSize>
                        <model>
                            <VectorListModel>
                                <e:String>'First'</e:String>
                                <e:String>'Second'</e:String>
                                <e:String>'Third'</e:String>
                                <e:String>'Four'</e:String>
                            </VectorListModel>
                        </model>
                    </JComboBox>
                </JPanel>
        </content>
</JWindow>

