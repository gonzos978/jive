<?xml version="1.0" encoding="UTF-8"?>
<JLabel xmlns="org.aswing"
        xmlns:geom="org.aswing.geom"
        xmlns:plaf="org.aswing.plaf.basic">
    <alignmentX>0.3</alignmentX>
    <alignmentY>0.7</alignmentY>
    <background>
        <ASColor>
            <rgb>0x123456</rgb>
            <alpha>0.5</alpha>
        </ASColor>
    </background>
    <backgroundDecorator>
        <SolidBackground/>
    </backgroundDecorator>
    <bounds>
        <geom:IntRectangle>
            <geom:x>10</geom:x>
            <geom:y>11</geom:y>
            <geom:width>100</geom:width>
            <geom:height>101</geom:height>
        </geom:IntRectangle>
    </bounds>
    <clipBounds>
        <geom:IntRectangle>
            <geom:x>11</geom:x>
            <geom:y>12</geom:y>
            <geom:width>98</geom:width>
            <geom:height>99</geom:height>
        </geom:IntRectangle>
    </clipBounds>
    <currentSize>
        <geom:IntDimension>
            <geom:width>95</geom:width>
            <geom:height>96</geom:height>
        </geom:IntDimension>
    </currentSize>
    <dragEnabled>true</dragEnabled>
    <dropTrigger>true</dropTrigger>
    <enabled>false</enabled>
    <focusable>false</focusable>
    <focusableSet>true</focusableSet>
    <font>
        <EmptyFont/>
    </font>
    <foreground>
        <ASColor>
            <rgb>0x654321</rgb>
            <alpha>0.1</alpha>
        </ASColor>
    </foreground>
    <location>
        <geom:IntPoint>
            <geom:x>15</geom:x>
            <geom:y>16</geom:y>
        </geom:IntPoint>
    </location>
    <maximumSize>
        <geom:IntDimension>
            <geom:width>500</geom:width>
            <geom:height>600</geom:height>
        </geom:IntDimension>
    </maximumSize>
    <minimumSize>
        <geom:IntDimension>
            <geom:width>10</geom:width>
            <geom:height>10</geom:height>
        </geom:IntDimension>
    </minimumSize>
    <mideground>
        <ASColor>
            <rgb>0x123123</rgb>
            <alpha>0.4</alpha>
        </ASColor>
    </mideground>
    <opaque>true</opaque>
    <preferredSize>
        <geom:IntDimension>
            <geom:width>100</geom:width>
            <geom:height>100</geom:height>
        </geom:IntDimension>
    </preferredSize>
    <styleTune>
        <StyleTune/>
    </styleTune>
    <toolTipText>'Tooltip text!'</toolTipText>
    <ui>
        <plaf:BasicLabelUI/>
    </ui>
    <uiElement>true</uiElement>
    <visibility>true</visibility>
    <text>'Hello world!!!'</text>
</JLabel>