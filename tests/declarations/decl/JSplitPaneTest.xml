<?xml version="1.0" encoding="UTF-8"?>
<JWindow xmlns="org.aswing" xmlns:geom="org.aswing.geom" xmlns:e="empty">
        <content>
                <JPanel>
                    <layout>
                            <FlowLayout>
                                    <align>org.aswing.FlowLayout.LEFT</align>
                                    <hgap>30</hgap>
                                    <vgap>30</vgap>
                                    <margin>true</margin>
                            </FlowLayout>
                    </layout>
                    <JSplitPane oneTouchExpandable="true" dividerSize="20">
                        <preferredSize>
                            <geom:IntDimension width="500" height="300"/>
                        </preferredSize>
                        <leftComponent>
                            <JLabel text="'left'"/>
                        </leftComponent>
                        <rightComponent>
                            <JLabel text="'right'"/>
                        </rightComponent>
                        <dividerLocation>100</dividerLocation>
                    </JSplitPane>
                </JPanel>
        </content>
</JWindow>

