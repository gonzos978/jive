<?xml version="1.0" encoding="UTF-8"?>
<JWindow xmlns="org.aswing" xmlns:geom="org.aswing.geom">
    <content>
        <JPanel>
            <layout>
                    <FlowLayout>
                            <align>org.aswing.FlowLayout.LEFT</align>
                            <hgap>30</hgap>
                            <vgap>30</vgap>
                            <margin>true</margin>
                    </FlowLayout>
            </layout>
            <JSlider>
                <preferredSize>
                    <geom:IntDimension width="300" height="50"/>
                </preferredSize>
                <minimum>0</minimum>
                <maximum>100</maximum>
                <majorTickSpacing>5</majorTickSpacing>
                <minorTickSpacing>10</minorTickSpacing>
                <snapToTicks>true</snapToTicks>
                <showValueTip>true</showValueTip>
                <paintTrack>true</paintTrack>
                <paintTicks>true</paintTicks>
            </JSlider>
            <JSlider>
                <preferredSize>
                    <geom:IntDimension width="100" height="300"/>
                </preferredSize>
                <minimum>0</minimum>
                <maximum>100</maximum>
                <majorTickSpacing>5</majorTickSpacing>
                <minorTickSpacing>10</minorTickSpacing>
                <snapToTicks>false</snapToTicks>
                <showValueTip>true</showValueTip>
                <paintTrack>true</paintTrack>
                <paintTicks>true</paintTicks>
                <orientation>org.aswing.JSlider.VERTICAL</orientation>
            </JSlider>
        </JPanel>
    </content>
</JWindow>